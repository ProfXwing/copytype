{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowJs": true,
    "module": "ES2015",
    "target": "ES2015",
    "lib": ["ES2021", "DOM"],
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "../../../dist/ts/renderer",
    "moduleResolution": "classic",
    "skipLibCheck": true
  },
  "include": ["**/*"],

  "files": ["window.d.ts"],
}


